{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "react/prop-types": "off",
        "no-use-before-define": "off",
        "no-shadow": "off",
        "no-param-reassign":"off",
        "prefer-const":"off",
        "no-await-in-loop":"off",
        "no-plusplus":"off"
    }
   
    
}
